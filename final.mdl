constants shiny_blue 0 0 0 0 0 0 0.7 0.5 0.9
constants dull_orange 0.8 0.8 0.8 0.1 0.4 0.2 0 0 0

//cylinder
push
move 100 0 0
move 0 500 0 b1
//rotate y 360 spinny
//rotate z 360 spinny
cone shiny_blue 0 0 0 30 75
pop

//cone
push
move 250 500 0
move 0 -500 0 b2
//rotate y 360 spinny
//rotate z 360 spinny
cylinder matte_white 0 0 0 30 75
pop

//pyramid
push
move 425 0 0
move 0 500 0 b1
//rotate y 360 spinny
//rotate z 360 spinny
pyramid shiny_teal 0 0 0 50 75
pop

//knobs
vary b1 0 33 0 1
vary b1 34 49 1 .5
//vary b1 50 74 .5 .5

vary b2 0 33 0 1
vary b2 34 49 1 .5
